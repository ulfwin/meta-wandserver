# This file allows custom wifi settings to be added
# Modify this file to your need, and rename it to
# "wpa_supplicant.conf-custom"

ctrl_interface=/var/run/wpa_supplicant
ctrl_interface_group=0
update_config=1

network={
        ssid="<network id>"
        scan_ssid=1
        key_mgmt=WPA-PSK
        psk="<password>"
}
